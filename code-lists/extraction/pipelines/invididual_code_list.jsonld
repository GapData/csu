    {
      "@graph": [
        {
          "@id": "http://localhost:8080/resources/pipelines/created-1500028294766/components/2b9e08b8-c1fd-4cf4-8153-3ec0f7e2a30f/configuration",
          "@type": [
            "http://plugins.linkedpipes.com/ontology/t-sparqlUpdate#Configuration"
          ],
          "http://plugins.linkedpipes.com/ontology/t-sparqlUpdate#query": [
            {
              "@value": "PREFIX dcterms: <http://purl.org/dc/terms/>\nPREFIX rdfs:    <http://www.w3.org/2000/01/rdf-schema#>\nPREFIX skos:    <http://www.w3.org/2004/02/skos/core#>\n\nINSERT {\n  ?class a rdfs:Class ;\n    rdfs:subClassOf skos:Concept ;\n    rdfs:seeAlso ?conceptScheme .\n}\nWHERE {\n  ?conceptScheme a skos:ConceptScheme ;\n    dcterms:identifier ?identifier .\n  BIND (iri(concat(\"https://csu.opendata.cz/slovník/\", ?identifier)) AS ?class)\n}\n;\n\nINSERT {\n  ?concept a ?class .\n}\nWHERE {\n  ?concept skos:inScheme/^rdfs:seeAlso ?class .\n}"
            }
